<div class="score-percent-row">
    <div class="score-percent" style="left: {{& percent}}%;">{{score}}</div>
</div>

<div class="score-progress-row">
    <div class="score-progress-frame">
        <div class="score-progress" style="width: {{& percent}}%; background: {{& background}};"></div>
    </div>
</div>
